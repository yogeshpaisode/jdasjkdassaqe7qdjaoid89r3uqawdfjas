<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.yogeshPaisode"
    version="1.0.4">
    <name>SecureValues</name>
    <description>SecureValues Plugin</description>
    <license>Apache 2.0</license>
    <keywords>cordova,coolest</keywords>
    <repo></repo>
    <issue></issue>
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="SecureValues" >
                <param name="android-package" value="SecureValues"/>
            </feature>
        </config-file>
        <source-file src="src/android/SecureValues.java" target-dir="src/" />
    </platform>
</plugin>
